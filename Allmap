-- =≠=======
-- อินฟินิตี้จั้ม
-- ===========

local humanoid
local function setupHumanoid(character)
    humanoid = character:WaitForChild("Humanoid")
    humanoid:GetPropertyChangedSignal("Jump"):Connect(function()
        if InfiniteJumpEnabled and humanoid.Jump then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
end

if Players.LocalPlayer.Character then
    setupHumanoid(Players.LocalPlayer.Character)
end

Players.LocalPlayer.CharacterAdded:Connect(function(char)
    setupHumanoid(char)
end)


-- ========
-- ฟังชั่นในแท็ปesp
-- ===========

-- ====
-- เลือด
-- ====

function HealthESP_Create(plr)
    if plr == lp or HealthESP[plr] then return end

    local gui = Instance.new("BillboardGui")
    gui.Name = "HealthESP"
    gui.Size = UDim2.fromOffset(64,10)
    gui.AlwaysOnTop = true
    gui.LightInfluence = 0
    gui.MaxDistance = 1e9

    local border = Instance.new("Frame", gui)
    border.Size = UDim2.fromScale(1,1)
    border.BackgroundColor3 = Color3.new(0,0,0)
    border.BorderSizePixel = 0
    Instance.new("UICorner", border).CornerRadius = UDim.new(1,0)

    local bg = Instance.new("Frame", border)
    bg.Position = UDim2.fromScale(0.01,0.12)
    bg.Size = UDim2.fromScale(0.98,0.76)
    bg.BackgroundColor3 = Color3.fromRGB(35,35,35)
    bg.BorderSizePixel = 0
    Instance.new("UICorner", bg).CornerRadius = UDim.new(1,0)

    local bar = Instance.new("Frame", bg)
    bar.Size = UDim2.fromScale(1,1)
    bar.BorderSizePixel = 0
    Instance.new("UICorner", bar).CornerRadius = UDim.new(1,0)

    HealthESP[plr] = {gui=gui, bar=bar}
end

function HealthESP_Update(plr)
    local d = HealthESP[plr]
    if not d then return end
    local char = plr.Character
    local hum = char and char:FindFirstChildOfClass("Humanoid")
    local head = char and char:FindFirstChild("Head")
    if not (hum and head) then return end

    d.gui.Parent = head
    local hp = math.clamp(hum.Health / hum.MaxHealth,0,1)
    d.bar.Size = UDim2.fromScale(hp,1)
    d.bar.BackgroundColor3 = Color3.fromHSV(hp*0.33,1,1)

    local dist = (cam.CFrame.Position - head.Position).Magnitude
    d.gui.Size = UDim2.fromOffset(64 + dist*0.05, 10)
end

function HealthESP_Remove(plr)
    if HealthESP[plr] then
        HealthESP[plr].gui:Destroy()
        HealthESP[plr] = nil
    end
end

-- =====
-- ชื่อ
-- ====

function NameESP_Create(plr)
    if plr == lp or NameESP[plr] then return end

    local gui = Instance.new("BillboardGui")
    gui.Name = "NameESP"
    gui.Size = UDim2.fromOffset(160,20)
    gui.AlwaysOnTop = true
    gui.LightInfluence = 0
    gui.MaxDistance = 1e9

    local txt = Instance.new("TextLabel", gui)
    txt.Size = UDim2.fromScale(1,1)
    txt.BackgroundTransparency = 1
    txt.TextScaled = true
    txt.Font = Enum.Font.GothamBold
    txt.TextStrokeTransparency = 0

    NameESP[plr] = {gui=gui, txt=txt}
end

function NameESP_Update(plr)
    local d = NameESP[plr]
    if not d then return end
    local head = plr.Character and plr.Character:FindFirstChild("Head")
    if not head then return end

    d.gui.Parent = head
    d.txt.Text = plr.Name

    if plr.Team then
        d.txt.TextColor3 = plr.Team.TeamColor.Color
    else
        d.txt.TextColor3 = Color3.new(1,1,1)
    end
end

function NameESP_Remove(plr)
    if NameESP[plr] then
        NameESP[plr].gui:Destroy()
        NameESP[plr] = nil
    end
end

-- ====
-- ระยะ
-- ====

function DistESP_Create(plr)
    if plr == lp or DistESP[plr] then return end

    local gui = Instance.new("BillboardGui")
    gui.Name = "DistESP"
    gui.Size = UDim2.fromOffset(90,18)
    gui.AlwaysOnTop = true
    gui.LightInfluence = 0
    gui.MaxDistance = 1e9

    local txt = Instance.new("TextLabel", gui)
    txt.Size = UDim2.fromScale(1,1)
    txt.BackgroundTransparency = 1
    txt.TextScaled = true
    txt.Font = Enum.Font.GothamBold
    txt.TextColor3 = Color3.new(1,1,1)
    txt.TextStrokeTransparency = 0

    DistESP[plr] = {gui=gui, txt=txt}
end

function DistESP_Update(plr)
    local d = DistESP[plr]
    if not d then return end
    local head = plr.Character and plr.Character:FindFirstChild("Head")
    if not head then return end

    d.gui.Parent = head
    local dist = math.floor((cam.CFrame.Position - head.Position).Magnitude)
    d.txt.Text = dist.."m"
end

function DistESP_Remove(plr)
    if DistESP[plr] then
        DistESP[plr].gui:Destroy()
        DistESP[plr] = nil
    end
end

-- ======
--  กันทับ
-- =====

function UpdateESPStack(plr)
    local y = 3.2

    if HEALTH_ENABLED and HealthESP[plr] then
        HealthESP[plr].gui.StudsOffset = Vector3.new(0,y,0)
        y = y + 0.9
    end

    if NAME_ENABLED and NameESP[plr] then
        NameESP[plr].gui.StudsOffset = Vector3.new(0,y,0)
        y = y + 0.9
    end

    if DIST_ENABLED and DistESP[plr] then
        DistESP[plr].gui.StudsOffset = Vector3.new(0,y,0)
    end
end

RunService.RenderStepped:Connect(function()
    for _,p in ipairs(Players:GetPlayers()) do
        if HEALTH_ENABLED then HealthESP_Update(p) end
        if NAME_ENABLED then NameESP_Update(p) end
        if DIST_ENABLED then DistESP_Update(p) end
        UpdateESPStack(p)
    end
end)

Players.PlayerAdded:Connect(function(p)
    if HEALTH_ENABLED then HealthESP_Create(p) end
    if NAME_ENABLED then NameESP_Create(p) end
    if DIST_ENABLED then DistESP_Create(p) end
end)

Players.PlayerRemoving:Connect(function(p)
    HealthESP_Remove(p)
    NameESP_Remove(p)
    DistESP_Remove(p)
end)


local MainTab = Window:Tab({Title = "MAIN", Icon = "user"})


MainTab:Toggle({
    Title = "walk Speed",
    Desc = "วิ่งไว",
    Default = false,
    Callback = function(v)
        toggleMovement(v)
    end
})


MainTab:Slider({
    Title = "Speed walk",
    Desc = "ความเร็ว",
    Step = 1,
    Value = {
        Min = 1,
        Max = 5,
        Default = 1
    },
    Callback = function(v)
        speedLevel = tonumber(v) or 1
        updateSpeed()
    end
})

MainTab:Toggle({
    Title = "Jump power",
    Desc = "โดดสูง",
    Default = false,
    Callback = function(v)
        toggleHighJump(v)
    end
})

MainTab:Slider({
    Title = "Jump Slider",
    Desc = "ปรับความสูงการกระโดด",
    Step = 5,
    Value = {
        Min = 50,
        Max = 500,
        Default = 50
    },
    Callback = function(v)
        jumpPowerValue = tonumber(v) or 50
    end
})

MainTab:Toggle({
    Title = "Infinite Jump",
    Desc = "กระโดดไม่จำกัด",
    Default = false,
    Callback = function(state)
        InfiniteJumpEnabled = state
    end
})


MainTab:Toggle({
    Title = "Noclip",
    Desc = "ทะลุกำแพง",
    Callback = function(state)
        frontNoclip = state
        if not frontNoclip then
            -- คืนค่า CanCollide ให้ทุกพาร์ทที่ถูกปรับ
            for part,_ in pairs(trackedParts) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
            trackedParts = {}
        end
    end
})



MainTab:Toggle({
    Title = "Bypass Fly",
    Desc = "บินแบบบายพาส",
    Default = API_Bypass["_CR.DayToDay2044_Fly"],
    Callback = function(v)
        API_Bypass["_CR.DayToDay2044_Fly"] = v
    end
})

MainTab:Slider({
    Title = "Fly Speed",
    Desc = "ความเร็วในการบิน",
    Step = 1,
    Value = {
        Min = 1,
        Max = 500,
        Default = API_Bypass["_CR.DayToDay2044_Speed"]
    },
    Callback = function(v)
        API_Bypass["_CR.DayToDay2044_Speed"] = tonumber(v) or 100
    end
})



